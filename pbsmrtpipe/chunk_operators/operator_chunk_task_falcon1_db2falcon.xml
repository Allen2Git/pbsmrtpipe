<?xml version="1.0" encoding="utf-8" ?>
<chunk-operator id="pbsmrtpipe.operators.chunk_task_falcon1_db2falcon">
    <task-id>falcon_ns.tasks.task_falcon1_db2falcon</task-id>
    <scatter>
        <scatter-task-id>pbfalcon.tasks.scatter_run_scripts_in_pickle_2</scatter-task-id>
        <chunks>
            <chunk out="$chunk.pickle_id" in="falcon_ns.tasks.task_falcon1_db2falcon:0"/>
            <chunk out="$chunk.txt_id" in="falcon_ns.tasks.task_falcon1_db2falcon:1"/>
        </chunks>
    </scatter>
    <!-- Define the Gather Mechanism -->
    <gather>
        <chunks>
           <chunk>
                <gather-task-id>pbcoretools.tasks.gather_txt</gather-task-id>
                <chunk-key>$chunk.txt_id</chunk-key>
                <task-output>falcon_ns.tasks.task_falcon1_db2falcon:0</task-output>
           </chunk>
        </chunks>
    </gather>
</chunk-operator>
