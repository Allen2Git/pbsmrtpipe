{
    "version": "0.1.0", 
    "driver": {
        "exe": "python -m pbsmrtpipe.tools_dev.fasta --emit-tool-contract ", 
        "env": {}
    }, 
    "tool_contract_id": "pbsmrtpipe.tasks.dev_fasta", 
    "tool_contract": {
        "task_type": "pbsmrtpipe.task_types.standard", 
        "resource_types": [], 
        "description": "Generate a random fasta file", 
        "schema_options": [
            {
                "$schema": "http://json-schema.org/draft-04/schema#", 
                "required": [
                    "pbsmrtpipe.task_options.dev_fasta.max_records"
                ], 
                "type": "object", 
                "properties": {
                    "pbsmrtpipe.task_options.dev_fasta.max_records": {
                        "default": 1000, 
                        "type": "integer", 
                        "description": "Max Records", 
                        "title": "Max Number of records to generate"
                    }
                }, 
                "title": "JSON Schema for pbsmrtpipe.task_options.dev_fasta.max_records"
            }
        ], 
        "output_types": [
            {
                "title": "Filtered Fasta", 
                "description": "Filtered Fasta by sequence length Fasta", 
                "default_name": "filtered.fasta", 
                "id": "fasta_out", 
                "file_type_id": "PacBio.FileTypes.Fasta"
            }
        ], 
        "_comment": "Created by v0.2.3", 
        "input_types": [
            {
                "description": "Sentinel file to trigger start", 
                "title": "Sentinel", 
                "id": "txt", 
                "file_type_id": "PacBio.FileTypes.txt"
            }
        ], 
        "nproc": 1, 
        "is_distributed": false, 
        "name": "Dev Fasta"
    }
}
