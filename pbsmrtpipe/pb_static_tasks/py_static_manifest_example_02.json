{
  "_comment": "Example Static Manifest",
  "_version": "0.1.0",
  "driver": {
    "_comment": "The execution layer will call '${exe} manifest.json' as the to_cmd",
    "exe": "python -m pbsmrtpipe.tools.static_driver_mock",
    "env": {}
  },
  "meta_task": {
    "task_id": "pbsmrtpipe.tasks.dev_static_txt_task",
    "task_type": "pbsmrtpipe.constants.local_task",
    "version": "0.2.1",
    "name": "Static Dev Txt",
    "description": "Example static task",
    "input_types": ["PacBio.FileTypes.txt"],
    "output_types": ["PacBio.FileTypes.txt"],
    "schema_options": {},
    "nproc": 1,
    "resource_types": ["$tmpdir", "$logfile"],
    "output_file_names": [["static_file","txt"]]
  }
}